{
    "name": "domisys/rabbit-mq-admin-toolkit",
    "description": "RabbitMQ administration toolkit",
    "license": "MIT",
    "require": {
        "symfony/console": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/yaml": "~2.4",
        "swarrot/swarrot": "~1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Bab\\RabbitMq": "src"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "bin": ["rabbit"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
