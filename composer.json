{
    "name": "odolbeau/rabbit-mq-admin-toolkit",
    "description": "RabbitMQ administration toolkit",
    "license": "MIT",
    "require": {
        "symfony/console": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/yaml": "~2.4",
        "swarrot/swarrot": "~1.3"
    },
    "autoload": {
        "psr-0": {
            "Bab\\RabbitMq": "src"
        }
    },
    "bin": ["rabbit"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.4"
    }
}
